[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/iyeon88
[access "refs/for/refs/*"]
	read = group user/iyeon88
	push = group user/iyeon88
	label-Code-Review = -2..+2 group user/iyeon88
	label-Verified = -1..+1 group user/iyeon88
	rebase = group user/iyeon88
	removeReviewer = group user/iyeon88
	submit = group user/iyeon88
[access "refs/heads/*"]
	read = group user/iyeon88
	create = group user/iyeon88
	push = +force group user/iyeon88
	pushMerge = group user/iyeon88
[access "refs/tags/*"]
	push = group user/iyeon88
	push = +force group user/iyeon88
